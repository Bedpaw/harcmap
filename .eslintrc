{
  "extends": [
    "plugin:vue/essential",
    "standard"
  ],
  "rules": {
    "no-undef": "off",
    "eqeqeq": "warn",
    "no-unused-vars": "warn",
    "no-useless-call": "warn",
    "no-useless-escape": "warn",
    "comma-dangle": [
      "error",
      "always-multiline"
    ],
    "indent": ["error", 2],
    "vue/html-indent": ["error", 2]
  }
}
