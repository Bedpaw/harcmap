{
  "appId": "pl.harcmap.app",
  "appName": "HarcMap",
  "webDir": "../public",
  "bundledWebRuntime": false,
  "server": {
    "hostname": "mobile.harcmap.pl",
    "androidScheme": "https"
  },
  "plugins": {
    "LocalNotifications": {
      "smallIcon": "ic_stat_icon_config_sample",
      "iconColor": "#07632C",
      "sound": "beep.wav"
    },
    "SplashScreen": {
      "launchAutoHide": false,
      "backgroundColor": "#07632C",
      "androidScaleType": "CENTER_CROP",
      "splashFullScreen": false,
      "splashImmersive": false,
      "layoutName": "launch_screen"
    }
  }
}
