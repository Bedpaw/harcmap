{
  "name": "harcmap",
  "version": "1.3.0",
  "description": "HarcMap",
  "main": "src/index.js",
  "scripts": {
    "build:dev": "npx webpack --config webpack.dev.js --watch",
    "build:prod": "npx webpack --config webpack.prod.js",
    "sass-watcher": "sass-watcher --config sass-watcher.json",
    "run:dev": "NODE_ENV=development node app.js",
    "run:prod": "NODE_ENV=production node app.js",
    "run:pm2": "pm2 start app.js",
    "run:mongodb": "docker run --name harcmap_mongo --rm -p 27017:27017 -v `pwd`/config/mongodb:/docker-entrypoint-initdb.d mongo",
    "run:mongodb:windows": "docker run --name harcmap_mongo --rm -p 27017:27017 -v %cd%/config/mongodb:/docker-entrypoint-initdb.d mongo",
    "fill-database": "NODE_ENV=development node bin/prepareEvent.js",
    "tests:restapi": "jest --config=jest-node.config.js --testPathPattern=restapi"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dbetka/harcmap.git"
  },
  "author": "dbetka",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dbetka/harcmap/issues"
  },
  "homepage": "https://github.com/dbetka/harcmap#readme",
  "engines": {
    "node": ">=12.9.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "@dbetka/wdk": "^0.1.5",
    "@dbetka/sass-watcher": "^0.1.5",
    "body-parser": "^1.19.0",
    "child_process": "^1.0.2",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "mongodb": "^3.5.3",
    "ncp": "^2.0.0",
    "node-fetch": "^2.6.1",
    "ol": "^6.6.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pm2": "^4.5.1",
    "sha3": "^2.1.1",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.4",
    "vue": "^2.6.14"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@dbetka/utils": "^0.1.3",
    "@dbetka/vue-material-icons": "^0.2.7",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.2.0",
    "eslint": "^7.31.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "^7.14.0",
    "url-loader": "^4.1.1",
    "html-webpack-plugin": "^5.3.2",
    "import-glob-loader": "^1.1.0",
    "jest": "^27.0.6",
    "js-cookie": "^2.2.1",
    "js-search": "^2.0.0",
    "json2csv": "^5.0.6",
    "moment": "^2.29.1",
    "nodemailer": "^6.6.3",
    "sass": "^1.35.2",
    "sass-loader": "^12.1.0",
